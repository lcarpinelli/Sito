
@{
    ViewData["Title"] = "Catalogo";
}

<h1>Catalogo</h1>
<br />

@foreach (var x in Model)
{
    <div class="row mb-3">

        <div class="col-md-4 ">
            @x
        </div>

        <div class="col-md-4 ">
            PREZZO
        </div>

        <form asp-controller="Catalogo" asp-action="Cata">
            <div class="col-md-4 ">
                <button type="submit"> DETTAGLI </button>
            </div>
        </form>

    </div>
}

